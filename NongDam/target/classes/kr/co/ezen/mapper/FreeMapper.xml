<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	
	<mapper namespace="kr.co.ezen.mapper.FreeMapper">
	
	<select id="findAll" resultType="kr.co.ezen.entity.Free">
	
	select * from free order by free_idx desc
	
	</select>
	
	<select id="findByidx" resultType="kr.co.ezen.entity.Free">
		select *from free where free_idx=#{free_idx}
	</select>
	
	
	<insert id="insert" parameterType="kr.co.ezen.entity.Free">
	insert into free (free_title, free_content, free_tag)
	values (#{free_title},#{free_content}, #{free_tag})
	</insert>
	
	</mapper>